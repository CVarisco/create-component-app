{
  "name": "create-component-app",
  "version": "1.0.0",
  "description": "script to create react component from terminal in easy way",
  "bin": {
    "create-component-app": "dist/index.js"
  },
  "repository": "https://github.com/CVarisco/create-component-app.git",
  "author": "Christian Varisco <vari.chri1993@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "pretest": "npm run build",
    "test": "jest --forceExit",
    "lint": "eslint src test",
    "flow": "flow check",
    "build": "babel src -d dist",
    "watch": "npm-watch",
    "try": "node ./dist/index.js"
  },
  "watch": {
    "test": "{src,templates,test}/*.js",
    "lint": "{src,templates,test}/*.js",
    "build": "{src,templates}/*.js"
  },
  "jest": {
    "testRegex": "test\\/[^/]+test\\.js$",
    "testEnvironment": "node"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "fs-extra": "^3.0.1",
    "inquirer": "^3.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-2": "^6.18.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-flowtype-errors": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.46.0",
    "jest-cli": "^20.0.0",
    "npm-watch": "^0.1.7",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.6.1",
    "suppose": "^0.6.2"
  }
}
